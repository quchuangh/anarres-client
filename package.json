    "@types/file-saver": "^2.0.1",
    "less-vars-to-js": "^1.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "theme": {
    "name": "pro",
    "version": "11.1.0"
  }
